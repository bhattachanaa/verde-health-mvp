{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "test-client.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/test-client.html",
      "dest": "/test-client.html"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}